{
  "main": "index.js",
  "name": "tree-store",
  "version": "0.0.0",
  "description": "Module for working with trees represented as arrays.",
  "author": "Demid Vasilyev <vasilyev.demid.e@gmail.com>",
  "scripts": {
    "start": "npm run build && npm run launch",
    "start:dev": "concurrently \"npm run build:dev\" \"npm run launch:dev\"",
    "build": "npx tsc",
    "launch": "node dist/index.js",
    "build:dev": "npx tsc --watch --preserveWatchOutput",
    "launch:dev": "nodemon dist/index.js",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@jest/globals": "^29.3.1",
    "babel-jest": "^29.3.1",
    "concurrently": "^7.5.0",
    "jest": "^29.3.1",
    "typescript": "^4.9.3"
  }
}
